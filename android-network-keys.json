    "environment_variables": {
      "network_config": {
        "ANDROID_DNS_MODE": {
          "description": "DNS resolution mode (values: 'local' for device-local resolution, 'remote' for network DNS servers)",
          "type": "string",
          "example": "local",
          "default": "local"
        },
        "ANDROID_SOCKET_*": {
          "description": "Socket file descriptor numbers for named sockets (e.g., ANDROID_SOCKET_zygote contains the FD number)",
          "type": "string",
          "example": "10",
          "default": ""
        },
        "HOSTNAME": {
          "description": "Device hostname",
          "type": "string",
          "example": "localhost",
          "default": "localhost"
        },
        "http_proxy": {
          "description": "HTTP proxy URL",
          "type": "string",
          "example": "http://proxy.example.com:8080",
          "default": ""
