        "net.http.proxy": {
          "description": "Global HTTP proxy",
          "type": "string",
          "example": "proxy.example.com:8080",
          "default": ""
        }
      },
      "vpn": {
        "vpn.version": {
          "description": "VPN version",
          "type": "string",
          "example": "1.0",
          "default": ""
