          "description": "TCP keepalive time in seconds",
          "type": "integer",
          "example": "7200",
          "default": "7200"
        },
        "/proc/sys/net/ipv4/tcp_keepalive_intvl": {
          "description": "TCP keepalive probe interval",
          "type": "integer",
          "example": "75",
          "default": "75"
        },
        "/proc/sys/net/ipv4/tcp_keepalive_probes": {
          "description": "Number of TCP keepalive probes",
          "type": "integer",
          "example": "9",
          "default": "9"
        },
        "/proc/sys/net/ipv4/tcp_fin_timeout": {
          "description": "TCP FIN timeout",
          "type": "integer",
          "example": "60",
          "default": "60"
        },
        "/proc/sys/net/ipv4/tcp_tw_reuse": {
          "description": "Enable TCP TIME_WAIT socket reuse",
          "type": "boolean",
          "example": "1",
          "default": "1"
        },
        "/proc/sys/net/ipv4/tcp_syncookies": {
          "description": "Enable TCP SYN cookies",
          "type": "boolean",
          "example": "1",
          "default": "1"
