          "default": "4096,87380,110208,4096,16384,110208"
        },
        "net.tcp.buffersize.wifi": {
          "description": "TCP buffer sizes for WiFi",
          "type": "string",
          "example": "524288,1048576,2097152,262144,524288,1048576",
          "default": "524288,1048576,2097152,262144,524288,1048576"
        },
        "net.tcp.buffersize.nr": {
          "description": "TCP buffer sizes for 5G NR",
          "type": "string",
          "example": "1048576,2097152,4194304,524288,1048576,2097152",
          "default": "1048576,2097152,4194304,524288,1048576,2097152"
        },
        "net.tcp.buffersize.lte": {
          "description": "TCP buffer sizes for LTE",
          "type": "string",
          "example": "524288,1048576,2097152,262144,524288,1048576",
          "default": "524288,1048576,2097152,262144,524288,1048576"
        },
        "net.tcp.buffersize.umts": {
          "description": "TCP buffer sizes for UMTS",
          "type": "string",
          "example": "58254,349525,1048576,58254,349525,1048576",
          "default": "58254,349525,1048576,58254,349525,1048576"
        },
        "net.tcp.buffersize.hspa": {
          "description": "TCP buffer sizes for HSPA",
          "type": "string",
          "example": "40778,244668,734003,16777,100663,301990",
          "default": "40778,244668,734003,16777,100663,301990"
        },
        "net.tcp.buffersize.edge": {
          "description": "TCP buffer sizes for EDGE",
          "type": "string",
          "example": "4093,26280,70800,4096,16384,70800",
          "default": "4093,26280,70800,4096,16384,70800"
